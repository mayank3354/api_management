generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ApiKey {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  key       String   @unique
  createdAt DateTime @default(now())
}

model ApiRequest {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  keyId     String   @db.ObjectId
  timestamp DateTime @default(now())
  endpoint  String
} 